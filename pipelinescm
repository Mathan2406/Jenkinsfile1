pipeline {
    agent any
    stages {
        stage('Clone Repository') {
            steps {
                git url: 'https://github.com/Mathan2406/Jenkinsfile1.git', branch: 'main'
            }
        }
        stage('Build') {
            steps {
                dir('root/jenkinsfile') {
                    script {
                        load 'Jenkinsfile1'
                    }
                }
            }
        }
    }
}
